javascript: async function QuillHax() {var url = window.location.href.split("student=");var uid = url[1];var payload = await fetch("https://www.quill.org/api/v1/active_activity_sessions/".concat(uid).concat(".json"));var payloadData = await payload.json();var qid = payloadData.currentQuestion.question;var answers = await fetch("https://cms.quill.org/questions/".concat(qid).concat("/responses"));var answersData = await answers.json();var final = answersData[0].text;for (let i = 0; i <= answersData.length; i = i + 1) {if (answersData[i].optimal == true) {document.getElementsByClassName("connect-text-area")[0].textContent = answersData[i].text;}}} QuillHax()
